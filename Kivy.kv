                    id: label_id0
                    size_hint: (0.5, 1)
                    # size: 100, 40
                    halign: "left"
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    text: root.Get_Param_Label(0)
                    # font_size: 26
                    size_hint_y: None
                    text_size: self.width, None
                    height: self.texture_size[1]

